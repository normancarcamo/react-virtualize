{
  "name": "react-virtualization",
  "version": "1.0.0",
  "description": "Virtualization in react.js",
  "main": "dist/server.js",
  "scripts": {
    "babel-watch": "./node_modules/.bin/babel --watch src --out-dir dist --ignore dist --only src --presets es2015,react --compact true --no-comments --no-highlight-code -D",
    "webpack-watch": "./node_modules/.bin/webpack --config webpack.config.js --progress --profile --colors --watch -v --display-error-details",
    "babel-compile": "./node_modules/.bin/babel src --out-dir dist --ignore dist --only src --presets es2015,react --compact true --no-comments --no-highlight-code -D",
    "webpack-compile": "./node_modules/.bin/webpack --config webpack.config.js --progress --profile --colors -v --display-error-details",
    "nodemon": "./node_modules/.bin/nodemon dist/server.js",
    "gulp": "gulp production"
  },
  "author": "Norman Carcamo",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "gulp": "^3.9.1",
    "nodemon": "^1.11.0",
    "webpack": "^1.13.3",
    "yargs": "^6.5.0"
  },
  "dependencies": {
    "express": "^4.14.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  }
}
